{
  "name": "fluent-memory-leak",
  "version": "1.0.0",
  "description": "A playground to reproduce a memory leak with fluent-ui components",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rimraf dist-prod && parcel build src/index.html --dist-dir dist-prod",
    "serve": "http-server dist-prod",
    "clean": "rimraf dist dist-prod .parcel-cache",
    "postinstall": "patch-package"
  },
  "author": "Andrew Stegmaier",
  "license": "ISC",
  "devDependencies": {
    "@parcel/validator-typescript": "^2.8.3",
    "@types/react": "^17.0.59",
    "@types/react-dom": "^17.0.20",
    "http-server": "^14.1.1",
    "parcel": "^2.8.3",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@fluentui/react": "^8.109.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
